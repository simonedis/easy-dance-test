{
  "application": {
    "name": "easy-dance",
    "endpoint": "http://localhost:3000"
  },
  "tables": {
    "address": {
      "autoBuild": {
        "tables": true,
        "forms": true
      },
      "setDefaultColumns": true,
      "endpoint": "addresses",
      "columns": {
        "street": {
          "type": "varchar",
          "minLength": 3,
          "maxLength": 350
        },
        "streetNumber": {
          "type": "varchar",
          "minLength": 3,
          "maxLength": 50
        },
        "cap": {
          "type": "varchar",
          "minLength": 3,
          "maxLength": 150
        },
        "city": {
          "type": "varchar",
          "minLength": 3,
          "maxLength": 150
        },
        "country": {
          "type": "varchar",
          "minLength": 3,
          "maxLength": 150
        },
        "lat": {
          "type": "bigint",
          "nullable": true
        },
        "lng": {
          "type": "bigint",
          "nullable": true
        }
      }
    },
    "club": {
      "setDefaultColumns": true,
      "columns": {
        "name": {
          "type": "varchar",
          "maxLength": 350
        },
        "cover": {
          "type": "varchar",
          "maxLength": 350
        },
        "phone": {
          "type": "varchar",
          "maxLength": 350
        },
        "email": {
          "type": "varchar",
          "maxLength": 350
        },
        "facebook": {
          "type": "varchar",
          "maxLength": 350
        },
        "instagram": {
          "type": "varchar",
          "maxLength": 350
        },
        "whatsapp": {
          "type": "varchar",
          "maxLength": 350
        }
      },
      "relations": {
        "owner": {
          "target": "user",
          "type": "one-to-one",
          "inverseSide": "club",
          "nullable": true,
          "joinColumn": true
        },
        "address": {
          "target": "address",
          "type": "one-to-one",
          "inverseSide": "club",
          "joinColumn": true
        },
        "parties": {
          "target": "party",
          "type": "one-to-many",
          "inverseSide": "club"
        },
        "warehouse": {
          "target": "warehouse",
          "type": "one-to-one",
          "inverseSide": "club"
        }
      }
    },
    "party": {
      "setDefaultColumns": true,
      "columns": {
        "title": {
          "type": "varchar"
        },
        "isWaitingList": {
          "type": "boolean"
        },
        "hidden": {
          "type": "boolean"
        },
        "from": {
          "type": "bigint"
        },
        "to": {
          "type": "bigint"
        },
        "cover": {
          "type": "varchar"
        }
      },
      "relations": {
        "club": {
          "type": "many-to-one",
          "target": "club",
          "inverseSide": "parties"
        },
        "address": {
          "type": "one-to-one",
          "target": "address"
        }
      }
    },
    "prive": {
      "setDefaultColumns": true,
      "columns": {
        "name": {
          "type": "varchar"
        },
        "capacity": {
          "type": "integer"
        },
        "minimumBill": {
          "type": "bigint",
          "nullable": true,
          "default": null
        },
        "enabled": {
          "type": "boolean",
          "default": true
        }
      }
    },
    "priveOrder": {
      "setDefaultColumns": true,
      "columns": {

      }
    },
    "userFollower": {
      "setDefaultColumns": true,
      "columns": {
      },
      "relations": {
        "follower": {
          "type": "many-to-one",
          "target": "user",
          "joinTable": true,
          "inverseSide": "followers"
        },
        "followed": {
          "type": "many-to-one",
          "target": "user",
          "joinTable": true,
          "inverseSide": "following"
        }
      }
    },
    "clubFollower": {
      "setDefaultColumns": true,
      "columns": {
      },
      "relations": {
        "user": {
          "type": "many-to-one",
          "target": "user",
          "joinTable": true,
          "inverseSide": "clubs"
        },
        "club": {
          "type": "many-to-one",
          "target": "club",
          "joinTable": true,
          "inverseSide": "following"
        }
      }
    },
    "user": {
      "setDefaultColumns": true,
      "name": "user",
      "autoBuild": {
        "tables": true,
        "forms": true
      },
      "columns": {
        "username": {
          "nullable": false,
          "name": "username",
          "type": "varchar",
          "minLength": 4,
          "unique": true
        },
        "email": {
          "type": "varchar",
          "unique": true
        },
        "password": {
          "type": "varchar",
          "nativeType": "string"
        }
      },
      "relations": {
        "followers": {
          "target": "userFollower",
          "inverseSide": "followed",
          "type": "one-to-many"
        },
        "followings": {
          "target": "userFollower",
          "inverseSide": "follower",
          "type": "one-to-many"
        },
        "club": {
          "type": "one-to-one",
          "nullable": true,
          "target": "club",
          "inverseSide": "owner"
        }
      }
    },
    "warehouse": {
      "setDefaultColumns": true,
      "columns": {

      },
      "relations": {
        "products": {
          "target": "product",
          "type": "one-to-many",
          "inverseSide": "warehouse"
        },
        "club": {
          "target": "club",
          "type": "one-to-one",
          "inverseSide": "warehouse"
        }
      }
    },
    "product": {
      "setDefaultColumns": true,
      "columns": {
        "name": {
          "type": "varchar"
        },
        "price": {
          "type": "bigint"
        },
        "qta": {
          "type": "bigint"
        }
      },
      "relations": {
        "warehouse": {
          "target": "warehouse",
          "type": "many-to-one",
          "inverseSide": "products"
        }
      }
    },
    "priceList": {
      "setDefaultColumns": true,
      "columns": {

      },
      "relations": {
        "party": {
          "target": "party",
          "type": "many-to-one",
          "joinTable": true,
          "inverseSide": "priceLists"
        }
      }
    }
  }
}
